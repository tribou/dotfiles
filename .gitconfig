[user]
	name = Aaron Tribou
	email = aaron.tribou@gmail.com
[core]
	editor = mvim -v
[credential]
[color]
	ui = true
	interactive = true
  diff = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = red
[color "diff"]
  meta = yellow
  frag = blue
  old = red bold
  new = green
[color "decorate"]
  head = reverse red
[color "status"]
  added = green
  changed = red
  untracked = cyan
[alias]
	tree = log --graph --abbrev-commit --date=local --date=short --pretty=format:\"%C(yellow)%h %C(cyan)%ad%C(auto)%d %Creset%s %C(blue)<%aN>\" --all
[push]
  default = simple

